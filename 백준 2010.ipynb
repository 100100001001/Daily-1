    "# 2010 플러그 https://www.acmicpc.net/problem/2010\n",
    "\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "multi_tap = 0\n",
    "for _ in range(n):\n",
    "    multi_tap += int(input())\n",
    "\n",
    "print(multi_tap - (n-1))"
