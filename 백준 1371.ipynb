    "import sys\r\n",
    "\r\n",
    "sentence = sys.stdin.read()\r\n",
    "alphabet = \"abcdefghijklmnopqrstuvwxyz\"\r\n",
    "dic = {}\r\n",
    "result = ''\r\n",
    "\r\n",
    "for i in alphabet:\r\n",
    "    dic[i] = sentence.count(i)\r\n",
    "\r\n",
    "for key, value in dic.items():\r\n",
    "    if value == max(dic.values()):\r\n",
    "        result += key\r\n",
    "\r\n",
    "print(result)"
